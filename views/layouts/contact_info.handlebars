                    <div class="same_line">
                        <button type="button" class="btn btn-primary btn-md bbt"  onclick="change_email()">CHANGE</button>
                        <div class="enter_email">
                            <form action="/change_email" method="post">
                                <input type="text" name="re_enter" autocomplete="off">
                                <div class="all_but">
                                    <button type="submit" class="btn btn-primary btn-md mbt">SUBMIT</button>
                                    <button type="button" class="btn btn-primary btn-md mbt close_b" onclick="closing_re_enter()">CLOSE</button>
                                </div>
                            </form>
                        </div>
                    </div>
